{
  "name": "currency-converted-data-fetcher",
  "version": "1.0.0",
  "description": "Fetches real-time currency conversion rates from OANDA",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "jest",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "prepare": "husky install"
  },
  "keywords": "currency,conversion,exchange-rate,oanda,typescript",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrew-Chen-Wang/currency-conversions.git"
  },
  "dependencies": {
    "axios": "^1.7.8"
  },
  "lint-staged": {
    "*.{js,ts}": "biome check --apply"
  }
}
